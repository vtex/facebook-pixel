{
  "vendor": "basedevmkp",
  "name": "facebook-pixel-teste",
  "version": "3.0.0",
  "title": "Facebook Pixel",
  "description": "Integrate with Facebook advertising tools.",
  "defaultLocale": "pt-BR",
  "builders": {
    "react": "3.x",
    "store": "0.x",
    "pixel": "0.x",
    "docs": "0.x"
  },
  "mustUpdateAt": "2019-10-01",
  "billingOptions": {
    "type": "billable",
    "support": {
      "email": "manoel.gumes@vtex.com.br"
    },
    "availableCountries": [
      "BRA",
      "ARG",
      "USA",
      "GBR"
    ],
    "plans": [
        {
            "id": "planoBrasil",
            "currency": "BRL",
            "price": {
                "metrics": [
                    {
                        "id": "SilverBR",
                        "ranges": [
                            {
                                "exclusiveFrom": 0,
                                "inclusiveTo": 25,
                                "multiplier": 5
                            },
                            {
                                "exclusiveFrom": 25,
                                "inclusiveTo": 100,
                                "multiplier": 2.5
                            },
                            {
                                "exclusiveFrom": 100,
                                "multiplier": 1
                            }
                        ]
                    },
                    {
                        "id": "GoldBR",
                        "ranges": [
                            {
                                "exclusiveFrom": 0,
                                "multiplier": 0.5
                            }
                        ]
                    }
                ]
            }
        },
        {
            "id": "planoGlobal",
            "currency": "USD",
            "price": {
                "metrics": [
                    {
                        "id": "SilverUS",
                        "ranges": [
                            {
                                "exclusiveFrom": 0,
                                "inclusiveTo": 25,
                                "multiplier": 1
                            },
                            {
                                "exclusiveFrom": 25,
                                "inclusiveTo": 100,
                                "multiplier": 0.5
                            },
                            {
                                "exclusiveFrom": 100,
                                "multiplier": 0.2
                            }
                        ]
                    },
                    {
                        "id": "GoldUS",
                        "ranges": [
                            {
                                "exclusiveFrom": 0,
                                "multiplier": 0.1
                            }
                        ]
                    }
                ]
            }
          
        }
      
    ]
  },
  "dependencies": {
    "vtex.pixel-interfaces": "1.x"
  },
  "settingsSchema": {
    "title": "Facebook Pixel",
    "type": "object",
    "properties": {
      "pixelId": {
        "title": "Facebook Pixel ID",
        "description": "Enter your facebook app ID",
        "type": "string"
      }
    }
  },
  "scripts": {
    "prereleasy": "bash lint.sh",
    "postreleasy": "vtex publish --verbose"
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
